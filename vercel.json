{
  "version": 2,
  "builds": [
    {
      "src": "frontend/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/app.ts"
    },
    {
      "src": "/favicon/apple-touch-icon.png",
      "dest": "frontend/src/public/favicon/apple-touch-icon.png"
    },
    {
      "src": "/favicon/favicon-32x32.png",
      "dest": "frontend/src/public/favicon/favicon-32x32.png"
    },
    {
      "src": "/favicon/favicon-16x16.png",
      "dest": "frontend/src/public/favicon/favicon-16x16.png"
    },
    {
      "src": "/favicon/site.webmanifest",
      "dest": "frontend/src/public/favicon/site.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ]
}